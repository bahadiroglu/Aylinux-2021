[derle]
betik     = cd debins
betik     = mkdir build
betik     = cd build
betik     = qmake ../
betik     = make

[pakur]
betik     = cd debins
betik     = mv debins $PKG/usr/bin/
betik     = mv /sources/mps $PKG/usr/bin/paketkur
betik     = chmod +x $PKG/usr/bin/paketkur
